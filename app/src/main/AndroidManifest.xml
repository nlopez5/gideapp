<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gideapp">

    <application
        android:icon="@mipmap/ic_launcher"
        android:label="@string/Gide"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gide">

        <activity
            android:name=".DismissActivity"
            android:theme="@style/Theme.Gide.Dismiss"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".RandomActivity"
            android:theme="@style/Theme.Gide.Random"
            android:exported="true">
        </activity>

    </application>

</manifest>